# defines structs describing neural network models
import Neural.layers as lyrs

trait Model:
  fn compile(inout self):
    ...

  fn eval(inout self):
    ...

  fn fit(inout self):
    ...

struct Sequential():

  var layers: Tuple[lyrs.Layer]

  fn __init__(inout self, layers: Tuple[lyrs.Layer]):
    self.layers = layers

  fn compile(inout self):
    pass

  fn eval(inout self):
    pass

  fn fit(inout self):
    pass
